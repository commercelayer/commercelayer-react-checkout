{
  "apiEndpoint": "https://your-organization-slug.commercelayer.io",
  "clientId": "",
  "clienrIdGod": "",
  "clientSecret": "",
  "scope" : "market:your-market",
  "record": true,
  "requests": [
    {
      "method": "GET",
      "url": "/api/shipping_methods**",
      "alias": "getShippingMethods"
    },
    {
      "method": "GET",
      "url": "/api/shipments/**",
      "alias": "getShipments"
    },
    {
      "method": "PATCH",
      "url": "/api/shipments/**",
      "alias": "patchShipments"
    },
    {
      "method": "GET",
      "url": "/api/orders/*/shipments/**",
      "alias": "getOrderShipments"
    },
    {
      "method": "GET",
      "url": "/api/orders/*/available_payment_methods*",
      "alias": "availablePaymentMethods"
    },
    {
      "method": "GET",
      "url": "/api/orders/*/line_items/**",
      "alias": "retrieveLineItems"
    },
    {
      "method": "GET",
      "url": "/api/orders/*",
      "alias": "getOrders"
    },
    {
      "method": "GET",
      "url": "/api/delivery_lead_times**",
      "alias": "deliveryLeadTimes"
    },
    {
      "method": "PATCH",
      "url": "/api/orders/*",
      "alias": "updateOrder"
    },
    {
      "method": "POST",
      "url": "/api/addresses",
      "alias": "createAddress"
    },
    {
      "method": "GET",
      "url": "/api/addresses/**",
      "alias": "getAddress"
    },
    {
      "method": "PATCH",
      "url": "/api/addresses/**",
      "alias": "updateAddress"
    },
    {
      "method": "GET",
      "url": "/api/customer_addresses**",
      "alias": "getCustomerAddresses"
    },
    {
      "method": "POST",
      "url": "/api/gift_cards",
      "alias": "insertGiftCard"
    },
    {
      "method": "POST",
      "url": "/api/stripe_payments",
      "alias": "stripePayments"
    },
    {
      "method": "GET",
      "url": "/api/payment_methods",
      "alias": "paymentMethods"
    },
    {
      "method": "GET",
      "url": "/api/orders/*/available_customer_payment_sources/**",
      "alias": "availableCustomerPaymentSources"
    }
  ]
}