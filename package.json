{
  "name": "commercelayer-react-checkout",
  "version": "1.4.3",
  "engines": {
    "node": ">=14.x",
    "yarn": "1.x"
  },
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "lint": "eslint ./ --ext .js,.jsx,.tsx,.ts",
    "lint:fix": "eslint ./ --ext .tsx,.ts --fix",
    "test": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commercelayer/commercelayer-react-checkout.git"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "author": {
      "name": "Matteo Alessani",
      "email": "matteo@commercelayer.io"
    },
    "license": "MIT",
    "bugs": {
      "url": "https://github.com/commercelayer/commercelayer-react-checkout/issues"
    },
    "homepage": "https://github.com/commercelayer/commercelayer-react-checkout#readme",
    "keywords": [
      "reactjs",
      "nextjs",
      "jamstack",
      "headless",
      "ecommerce",
      "api",
      "checkout",
      "commercelayer"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      [
        "@semantic-release/github",
        {
          "addReleases": "top"
        }
      ]
    ]
  },
  "dependencies": {
    "@commercelayer/react-components": "^3.5.1",
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@headlessui/react": "^1.4.3",
    "@rollbar/react": "^0.9.0",
    "classnames": "^2.3.1",
    "jwt-decode": "^3.1.2",
    "next": "12.0.10",
    "next-i18next": "^10.2.0",
    "pure-md5": "^0.1.13",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-gtm-module": "^2.0.11",
    "rollbar": "^2.24.0",
    "styled-components": "^5.3.3",
    "swr": "^1.2.1",
    "twin.macro": "2.8.2"
  },
  "devDependencies": {
    "@commercelayer/cypress-vcr": "^1.1.0",
    "@faker-js/faker": "6.0.0-alpha.5",
    "@next/bundle-analyzer": "^12.0.10",
    "@next/eslint-plugin-next": "^12.0.10",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^9.0.0",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@types/node": "^17.0.16",
    "@types/react": "^17.0.39",
    "@types/react-gtm-module": "^2.0.1",
    "@types/styled-components": "^5.1.22",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "autoprefixer": "^10.4.2",
    "cypress": "^9.4.1",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-tailwind": "^0.2.1",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "stylelint": "^14.4.0",
    "stylelint-config-standard": "^25.0.0",
    "tailwindcss": "^3.0.19",
    "typescript": "^4.5.5"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  },
  "cacheDirectories": [
    ".next/cache"
  ]
}